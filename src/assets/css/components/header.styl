.info-bar
    background var(--color_default)
    color #fff
    display flex
    justify-content: center
    align-items center
    font-size 14px
    font-weight 400
    padding 10px 0

    a
        text-decoration underline !important

.new-arrival

    .product-lists
        display flex
        width 100%
        position relative

        .button_payment
            display flex
            justify-content space-between
            margin-top 15px
            width 100%  
            button
                height 43px
                border-width 2px
                border-radius 8px
                border-style solid
                cursor pointer

                &.add_to_cart
                    width 58px  
                    background-color #fff
                    color var(--default_bg)
                    border 2px solid #ff0080
                    font-weight: 700;
                    font-size: 14px
                    display flex
                    justify-content space-between
                    align-items center
                    padding 12px
                    margin 0

                    &:hover
                        background-color var(--default_bg)
                        border-color var(--default_bg)
                        color #fff

                        i
